{
  "name": "thefurnhub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true react-app-rewired start",
    "build": "DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false react-app-rewired build",
    "postbuild": "rimraf build/static/**/*.map",
    "test": "DISABLE_ESLINT_PLUGIN=true react-app-rewired test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx ./src",
    "clear-all": "rm -rf build node_modules",
    "re-start": "rm -rf build node_modules && yarn install && yarn start",
    "re-build": "rm -rf build node_modules && yarn install && yarn build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@iconify/react": "^4.1.1",
    "@mui/lab": "^5.0.0-alpha.155",
    "@mui/material": "^5.14.20",
    "@mui/styles": "^5.14.20",
    "@mui/x-date-pickers": "^6.18.4",
    "@reduxjs/toolkit": "^2.0.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "classnames": "^2.5.1",
    "date-fns": "^2.30.0",
    "formik": "^2.4.6",
    "i18next": "^23.7.8",
    "i18next-browser-languagedetector": "^7.2.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.3",
    "react-i18next": "^13.5.0",
    "react-image-gallery": "^1.3.0",
    "react-input-mask": "^2.0.4",
    "react-medium-image-zoom": "^5.1.9",
    "react-multi-carousel": "^2.8.4",
    "react-rating-stars-component": "^2.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "react-secure-storage": "^1.3.2",
    "simplebar-react": "^3.2.4",
    "typescript": "^5.4.3",
    "yarn": "^1.22.21",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "browserify": "^17.0.0",
    "customize-cra": "^1.0.0",
    "envify": "^4.1.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "react-app-rewired": "^2.2.1",
    "terser": "^5.30.0",
    "uglifyify": "^5.0.2"
  }
}
